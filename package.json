{
  "name": "dynamic-object-yadab",
  "version": "1.0.7",
  "description": "Manage a list by dividing it in a dynamic object depending on that list's specific item",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon",
    "start:prod": "npm run build && node build/index.js",
    "build": "rimraf ./build && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel ./src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "tsc2js": "tsc",
    "babel:v": "babel --version"
  },
  "keywords": [
    "npm",
    "react",
    "dynamic-object",
    "list-to-object",
    "key-list-pair"
  ],
  "author": "Yadab Sd",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^16.4.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "node": "^16.6.0"
  }
}
